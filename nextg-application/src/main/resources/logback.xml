<?xml version="1.0" encoding="UTF-8"?>
<configuration scan="true">
	<appender name="STDOUT" class="com.github.hmld.framework.system.log.SysLoggerAppender"> 
		<encoder>
			<pattern>
				%d{yyyy-MM-dd HH:mm:ss} [%level] - %msg%n
			</pattern>
		</encoder>
	</appender>
	
	<root level="debug">
		<appender-ref ref="STDOUT" />
	</root>
</configuration>